{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Node.js server process",
      "address": "localhost",
      "port": 9229,
      "protocol": "inspector",
      "sourceMaps": true,
      "restart": true,
      "localRoot": "${workspaceFolder}/server",
      "remoteRoot": "/service"
    },
    {
      "name": "Attach to python server process",
      "type": "python",
      "request": "attach",
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}/alternatives/flask-server",
          "remoteRoot": "/service"
        }
      ],
      "port": 9229,
      "host": "localhost"
    }
  ]
}
