{
  "name": "full-stack-template-server",
  "private": true,
  "scripts": {
    "unit": "jest --rootDir=src --passWithNoTests -t",
    "test": "jest --rootDir=test --passWithNoTests -t",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development ts-node-dev --respawn --inspect=0.0.0.0:9229 -- index.ts | bunyan -l debug",
    "lint": "eslint './src/**/*.ts' --ignore-path .eslintignore --quiet",
    "typecheck": "tsc -p . --noEmit",
    "build:test": "tsc --build tsconfig.test.json",
    "build:prod": "NODE_ENV=production tsc --build tsconfig.json",
    "start:prod": "NODE_ENV=production npm run build:prod && node build/index.js",
    "generate:test-gql": "rm -rf ./test/graphql/generated/* && mkdir -p ./test/graphql/generated/_deep/queries && mkdir -p ./test/graphql/generated/_shallow/mutations && gqlg --schemaFilePath ./shared/schema.gql --destDirPath ./test/graphql/generated/_deep --depthLimit 8 && gqlg --schemaFilePath ./shared/schema.gql --destDirPath ./test/graphql/generated/_shallow --depthLimit 3 && mv ./test/graphql/generated/_deep/queries ./test/graphql/generated && mv ./test/graphql/generated/_shallow/mutations ./test/graphql/generated && rm -rf ./test/graphql/generated/_deep && rm -rf ./test/graphql/generated/_shallow",
    "generate:test-sdk": "graphql-codegen --config codegen.yml"
  },
  "_comment1": "You may want to remove @sentry/node if you don't use Sentry",
  "_comment2": "Required by REST API only: koa-joi-router",
  "_comment3": "Required by REST API docs only: mustache, showdown",
  "_comment4": "Required by GraphQL API only: apollo-server-koa, graphql, type-graphql",
  "dependencies": {
    "@apollo/server": "4.10.1",
    "@as-integrations/koa": "1.1.1",
    "@aws-sdk/client-s3": "3.525.0",
    "@aws-sdk/client-secrets-manager": "3.525.0",
    "@aws-sdk/s3-request-presigner": "3.525.0",
    "@graphql-tools/schema": "9.0.17",
    "@hapi/boom": "10.0.1",
    "@sentry/node": "7.105.0",
    "@types/koa-bodyparser": "4.3.12",
    "axios": "1.6.7",
    "bcrypt": "5.1.1",
    "bunyan": "1.8.15",
    "class-validator": "0.14.1",
    "eslint-plugin-n": "15.6.1",
    "graphql": "16.8.1",
    "graphql-depth-limit": "1.1.0",
    "graphql-ws": "5.15.0",
    "joi-to-swagger": "6.2.0",
    "jsonwebtoken": "9.0.2",
    "koa": "2.15.0",
    "koa-bodyparser": "4.4.1",
    "koa-joi-router": "8.0.0",
    "koa-jwt": "4.0.4",
    "koa-qs": "3.0.0",
    "module-alias": "2.2.3",
    "mustache": "4.2.0",
    "node-gyp": "10.0.1",
    "openapi-types": "12.1.3",
    "pg-promise": "10.15.4",
    "reflect-metadata": "0.2.1",
    "serverless-http": "3.2.0",
    "showdown": "2.1.0",
    "stack-trace": "0.0.10",
    "type-graphql": "2.0.0-beta.1",
    "typedi": "0.10.0",
    "uuid": "9.0.1",
    "ws": "8.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-graphql-request": "4.3.3",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "2.4.3",
    "@types/bunyan": "1.8.11",
    "@types/graphql-depth-limit": "1.1.6",
    "@types/jest": "29.5.12",
    "@types/joi": "17.2.3",
    "@types/koa": "2.15.0",
    "@types/koa-joi-router": "8.0.7",
    "@types/koa-qs": "2.0.3",
    "@types/koa-router": "7.4.8",
    "@types/mustache": "4.2.5",
    "@types/node": "20.11.24",
    "@types/pg": "8.11.2",
    "@types/showdown": "2.0.6",
    "@types/stack-trace": "0.0.33",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "gql-generator": "https://github.com/TaitoUnited/gql-generator/archive/refs/tags/v2.1.1-taito.tar.gz",
    "jest": "29.7.0",
    "prettier": "2.8.3",
    "ts-jest": "29.1.2",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.5"
  }
}
