{
  "name": "full-stack-template-server",
  "private": true,
  "scripts": {
    "unit": "MODE=unit vitest run",
    "unit:watch": "MODE=unit vitest watch",
    "test": "MODE=api vitest run",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development tsx watch index.ts --inspect=0.0.0.0:9229 | bunyan -o short -l debug",
    "lint": "eslint './src/**/*.ts' --quiet",
    "typecheck": "tsc -p . --noEmit",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:migrate:drop": "drizzle-kit drop",
    "db:migrate:generate": "drizzle-kit generate",
    "db:seed": "tsx ./src/db/seed/index.ts",
    "graphql:generate": "gql.tada generate output",
    "graphql:check": "gql.tada check",
    "build:prod": "NODE_ENV=production tsc --build tsconfig.json && tsc-alias -p tsconfig.json",
    "start:prod": "NODE_ENV=production npm run build:prod && node --experimental-require-module build/index.js"
  },
  "dependencies": {
    "@apollo/server": "4.11.0",
    "@as-integrations/fastify": "2.1.1",
    "@aws-sdk/client-s3": "3.637.0",
    "@aws-sdk/client-secrets-manager": "3.637.0",
    "@aws-sdk/s3-request-presigner": "3.637.0",
    "@escape.tech/graphql-armor": "3.0.1",
    "@fastify/cookie": "9.4.0",
    "@fastify/multipart": "8.3.0",
    "@fastify/type-provider-typebox": "4.1.0",
    "@lucia-auth/adapter-drizzle": "1.1.0",
    "@node-rs/argon2": "1.8.3",
    "@pothos/core": "4.2.0",
    "@pothos/plugin-scope-auth": "4.1.0",
    "@pothos/plugin-simple-objects": "4.1.0",
    "@sentry/node": "8.26.0",
    "@sinclair/typebox": "0.33.7",
    "bunyan": "1.8.15",
    "drizzle-orm": "0.33.0",
    "fastify": "4.28.1",
    "fastify-plugin": "4.5.1",
    "graphql": "16.9.0",
    "graphql-scalars": "1.23.0",
    "graphql-ws": "5.16.0",
    "ky": "1.7.1",
    "lucia": "3.2.0",
    "pg": "8.12.0",
    "query-string": "9.1.0",
    "uuid": "10.0.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.1.1",
    "@eslint/js": "9.9.0",
    "@types/bcrypt": "5.0.2",
    "@types/bunyan": "1.8.11",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.5.0",
    "@types/pg": "8.11.6",
    "@types/uuid": "10.0.0",
    "@types/ws": "8.5.12",
    "@typescript-eslint/parser": "8.2.0",
    "drizzle-kit": "0.24.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import-x": "3.1.0",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "7.1.0",
    "gql.tada": "1.8.6",
    "graphql-request": "6.1.0",
    "prettier": "3.3.3",
    "tsc-alias": "1.8.10",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "typescript-eslint": "8.2.0",
    "vitest": "2.0.5"
  },
  "engines": {
    "node": ">=20"
  }
}
