{
  "name": "full-stack-template-server",
  "private": true,
  "scripts": {
    "preinstall": "if [ -f package-lock.json ]; then npx npm-force-resolutions; fi",
    "unit": "jest src/ --passWithNoTests -t",
    "test": "jest test/ --passWithNoTests -t",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development ts-node-dev --respawn $( ( [ $HOST_OS = macos ] || [ $HOST_OS = windows ] ) && [ ! $DC_COMMAND ] && echo --poll) --inspect=0.0.0.0:9229 -- index.ts | bunyan -l debug",
    "lint": "tsc -p . --noEmit && eslint ./src/**/*.ts --ignore-path .eslintignore --quiet",
    "build:test": "tsc --build tsconfig.test.json",
    "build:prod": "NODE_ENV=production tsc --build tsconfig.json",
    "start:prod": "NODE_ENV=production npm run build:prod && node build/index.js"
  },
  "dependencies": {
    "@sentry/node": "^5.19.0",
    "aws-sdk": "^2.709.0",
    "axios": "^0.19.2",
    "@hapi/boom": "^9.1.0",
    "bunyan": "^1.8.14",
    "koa": "^2.13.0",
    "koa-joi-router": "^6.0.2",
    "koa-qs": "^3.0.0",
    "lodash": "^4.17.15",
    "mustache": "^4.0.1",
    "pg-promise": "^10.5.7",
    "serverless-http": "^2.5.0",
    "showdown": "^1.9.1",
    "stack-trace": "0.0.10",
    "uuid": "^8.2.0"
  },
  "resolutions": {
    "qs": "~6.3.2"
  },
  "devDependencies": {
    "@types/bunyan": "1.8.6",
    "@types/jest": "^26.0.3",
    "@types/joi": "^14.3.4",
    "@types/koa": "^2.11.3",
    "@types/koa-qs": "2.0.0",
    "@types/koa-router": "^7.4.1",
    "@types/lodash": "^4.14.157",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.0.14",
    "@types/pg": "^7.11.0",
    "@types/showdown": "1.9.3",
    "@types/stack-trace": "0.0.29",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node-dev": "1.0.0-pre.52",
    "typescript": "^3.9.6"
  }
}
