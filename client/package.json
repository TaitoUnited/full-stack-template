{
  "name": "full-stack-template-client",
  "scripts": {
    "install:clean": "rm -rf node_modules && npm install",
    "pretty-quick": "pretty-quick",
    "lint": "tsc --noEmit && eslint './src/**/*.{ts,tsx}' --ignore-path .eslintignore --quiet",
    "unit": "BABEL_ENV=test jest src/ --passWithNoTests -t",
    "test": "echo TODO run tests but not cypress",
    "cypress:run": "cypress run -s test/integration/**/${test_name}.spec.js",
    "cypress:open": "cypress open",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "ANALYZE=true npm run build && source-map-explorer 'build/assets/*.js' --no-border-checks",
    "lang:add-locale": "lingui add-locale",
    "lang:extract": "lingui extract",
    "lang:compile": "lingui compile",
    "uikit:preview": "start-storybook -p 6006",
    "generate:graphql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.13",
    "@lingui/macro": "4.1.2",
    "@lingui/react": "4.1.2",
    "@loadable/component": "5.15.3",
    "@sentry/browser": "7.50.0",
    "apollo3-cache-persist": "0.14.1",
    "date-fns": "2.29.3",
    "framer-motion": "10.12.4",
    "graphql": "16.5.0",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-aria": "3.24.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "react-icons": "4.8.0",
    "react-merge-refs": "1.1.0",
    "react-router": "6.10.0",
    "react-router-dom": "6.10.0",
    "react-stately": "3.22.0",
    "react-use": "17.4.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@lingui/cli": "4.1.2",
    "@lingui/swc-plugin": "4.0.0",
    "@lingui/vite-plugin": "4.1.2",
    "@optimize-lodash/rollup-plugin": "4.0.3",
    "@rollup/plugin-strip": "3.0.2",
    "@storybook/addon-actions": "6.5.5",
    "@storybook/addon-docs": "6.5.5",
    "@storybook/addon-essentials": "6.5.5",
    "@storybook/addon-links": "6.5.5",
    "@storybook/addons": "6.5.5",
    "@storybook/builder-webpack5": "6.5.5",
    "@storybook/manager-webpack5": "6.5.5",
    "@storybook/react": "6.5.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.1",
    "@types/loadable__component": "5.13.4",
    "@types/lodash": "4.14.194",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitejs/plugin-react": "4.0.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-plugin-macros": "3.1.0",
    "core-js": "3.30.1",
    "cypress": "9.3.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "pg-promise": "10.11.1",
    "prettier": "2.8.8",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "source-map-explorer": "2.5.3",
    "storybook-dark-mode": "1.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite": "4.3.3",
    "vite-plugin-favicon2": "1.1.5",
    "vite-tsconfig-paths": "4.2.0"
  },
  "babel_comment": "Babel is only needed for LinguiJS message extraction and testing with Jest",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      "macros"
    ]
  },
  "lingui": {
    "locales": [
      "en",
      "fi"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po",
    "compileNamespace": "ts"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^~constants(.*)$": "<rootDir>/src/constants$1",
      "^~services(.*)$": "<rootDir>/src/services$1",
      "^~shared(.*)$": "<rootDir>/shared$1",
      "^~graphql(.*)$": "<rootDir>/src/graphql$1",
      "^~uikit(.*)$": "<rootDir>/src/components/uikit$1",
      "^~components(.*)$": "<rootDir>/src/components$1",
      "^~utils(.*)$": "<rootDir>/src/utils$1"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
