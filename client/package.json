{
  "name": "full-stack-template-client",
  "main": "src/index.js",
  "scripts": {
    "install:clean": "rm -rf node_modules && npm install",
    "pretty-quick": "pretty-quick",
    "lint": "tsc --noEmit && eslint ./src/**/*.{ts,tsx} --ignore-path .eslintignore --quiet",
    "unit": "BABEL_ENV=test jest src/ --passWithNoTests -t",
    "test": "echo TODO run tests but not cypress",
    "cypress:run": "cypress run -s test/integration/**/${test_name}.spec.js",
    "cypress:open": "cypress open",
    "start": "NODE_ENV=development webpack serve --mode development",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "analyze": "ANALYZE_BUNDLE=true npm run build",
    "lang:add-locale": "lingui add-locale",
    "lang:extract": "lingui extract",
    "lang:compile": "lingui compile",
    "uikit:preview": "start-storybook -p 6006",
    "generate:graphql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "~3.5.8",
    "@lingui/macro": "~3.13.2",
    "@lingui/react": "~3.13.2",
    "@loadable/component": "~5.15.2",
    "@sentry/browser": "~6.17.3",
    "apollo3-cache-persist": "0.13.0",
    "axios": "0.25.0",
    "date-fns": "~2.28.0",
    "framer-motion": "^6.2.3",
    "graphql": "~15.8.0",
    "history": "~5.2.0",
    "lodash": "~4.17.21",
    "make-plural": "~7.0.0",
    "react": "~17.0.2",
    "react-aria": "~3.12.0",
    "react-dom": "~17.0.2",
    "react-hot-toast": "~2.2.0",
    "react-icons": "~4.3.1",
    "react-merge-refs": "~1.1.0",
    "react-router": "~6.2.1",
    "react-router-dom": "~6.2.1",
    "react-stately": "~3.11.0",
    "react-use": "~17.3.2",
    "styled-components": "~5.3.3",
    "workbox-window": "~6.4.2"
  },
  "devDependencies": {
    "@babel/cli": "~7.16.8",
    "@babel/core": "~7.16.12",
    "@babel/plugin-proposal-class-properties": "~7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "~7.16.7",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.16.11",
    "@babel/preset-react": "~7.16.7",
    "@babel/preset-typescript": "~7.16.7",
    "@graphql-codegen/cli": "~2.4.0",
    "@graphql-codegen/typescript": "~2.4.2",
    "@graphql-codegen/typescript-operations": "~2.2.2",
    "@graphql-codegen/typescript-react-apollo": "~3.2.4",
    "@lingui/cli": "~3.13.2",
    "@lingui/loader": "~3.13.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@storybook/addon-actions": "~6.4.16",
    "@storybook/addon-docs": "~6.4.16",
    "@storybook/addon-essentials": "~6.4.16",
    "@storybook/addon-links": "~6.4.16",
    "@storybook/addons": "~6.4.16",
    "@storybook/builder-webpack5": "~6.4.16",
    "@storybook/manager-webpack5": "~6.4.16",
    "@storybook/react": "~6.4.16",
    "@testing-library/jest-dom": "~5.16.1",
    "@testing-library/react": "~12.1.2",
    "@testing-library/user-event": "~13.5.0",
    "@types/jest": "~27.4.0",
    "@types/loadable__component": "~5.13.4",
    "@types/lodash": "~4.14.178",
    "@types/node": "^17.0.13",
    "@types/react": "~17.0.38",
    "@types/react-dom": "~17.0.11",
    "@types/react-router-dom": "~5.3.3",
    "@types/styled-components": "~5.1.21",
    "@typescript-eslint/eslint-plugin": "~4.30.0",
    "@typescript-eslint/parser": "~4.30.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-loader": "~8.2.3",
    "babel-plugin-dynamic-import-node": "~2.3.3",
    "babel-plugin-lodash": "~3.3.4",
    "babel-plugin-macros": "~3.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "babel-plugin-syntax-class-properties": "~6.13.0",
    "babel-plugin-transform-remove-console": "~6.9.4",
    "clean-webpack-plugin": "~4.0.0",
    "core-js": "~3.20.3",
    "css-loader": "~6.5.1",
    "cypress": "9.3.1",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-config-standard": "~16.0.3",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-lodash": "~7.3.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-promise": "~5.1.0",
    "eslint-plugin-react": "~7.28.0",
    "eslint-plugin-react-hooks": "~4.3.0",
    "favicons": "~6.2.2",
    "favicons-webpack-plugin": "~5.0.2",
    "fork-ts-checker-webpack-plugin": "~6.5.0",
    "html-webpack-plugin": "~5.5.0",
    "jest": "~27.4.7",
    "lodash-webpack-plugin": "0.11.6",
    "mini-css-extract-plugin": "~2.5.3",
    "pg-promise": "~10.11.1",
    "prettier": "~2.5.1",
    "react-refresh": "0.11.0",
    "source-map-loader": "~3.0.1",
    "storybook-dark-mode": "~1.0.8",
    "style-loader": "~3.3.1",
    "typescript": "~4.5.5",
    "webpack": "~5.67.0",
    "webpack-bundle-analyzer": "~4.5.0",
    "webpack-cli": "~4.9.2",
    "webpack-dev-server": "~4.7.3",
    "workbox-webpack-plugin": "~6.4.2"
  },
  "lingui": {
    "locales": [
      "en",
      "fi"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po",
    "compileNamespace": "ts"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^~constants(.*)$": "<rootDir>/src/constants$1",
      "^~services(.*)$": "<rootDir>/src/services$1",
      "^~shared(.*)$": "<rootDir>/shared$1",
      "^~graphql(.*)$": "<rootDir>/src/graphql$1",
      "^~uikit(.*)$": "<rootDir>/src/components/uikit$1",
      "^~components(.*)$": "<rootDir>/src/components$1",
      "^~utils(.*)$": "<rootDir>/src/utils$1"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
