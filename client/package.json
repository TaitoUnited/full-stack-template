{
  "name": "full-stack-template-client",
  "scripts": {
    "prepare": "npm run styled-system:codegen",
    "postinstall": "patch-package",
    "install:clean": "rm -rf node_modules && npm install",
    "pretty-quick": "pretty-quick",
    "lint": "tsc --noEmit && eslint './src/**/*.{ts,tsx}' --ignore-path .eslintignore --quiet",
    "unit": "echo TODO make this work in container vitest run ./src/**/*.test.{ts,tsx}",
    "unit:coverage": "vitest run ./src/**/*.test.{ts,tsx} --coverage",
    "test": "echo TODO run tests but not cypress",
    "cypress:run": "if [ -z ${test_name} ]; then cypress run; else cypress run -s test/integration/**/${test_name}.spec.ts; fi",
    "cypress:open": "cypress open",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "ANALYZE=true npm run build && source-map-explorer 'build/assets/*.js' --no-border-checks",
    "lang:add-locale": "lingui add-locale",
    "lang:extract": "lingui extract",
    "lang:compile": "lingui compile",
    "uikit:preview": "storybook dev -p 6006",
    "uikit:build": "storybook build",
    "design-system:sync": "npm run design-system:tokenize && npm run design-system:codegen && npm run design-system:format",
    "design-system:tokenize": "npx figmage tokenize --env .env.figma",
    "design-system:codegen": "npx figmage codegen --env .env.figma",
    "design-system:format": "prettier --write \"src/design-system/*.ts\"",
    "styled-system:codegen": "panda codegen --clean",
    "styled-system:debug": "panda debug",
    "generate:graphql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "3.7.13",
    "@lingui/macro": "4.2.1",
    "@lingui/react": "4.2.1",
    "@loadable/component": "5.15.3",
    "@sentry/browser": "7.50.0",
    "apollo3-cache-persist": "0.14.1",
    "date-fns": "2.29.3",
    "framer-motion": "10.12.4",
    "graphql": "16.5.0",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-aria": "3.24.0",
    "react-aria-components": "1.0.0-alpha.4",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "react-merge-refs": "1.1.0",
    "react-router": "6.10.0",
    "react-router-dom": "6.10.0",
    "react-stately": "3.22.0",
    "react-use": "17.4.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@lingui/cli": "4.2.1",
    "@lingui/swc-plugin": "4.0.2",
    "@lingui/vite-plugin": "4.2.1",
    "@optimize-lodash/rollup-plugin": "4.0.3",
    "@pandacss/dev": "0.5.0",
    "@rollup/plugin-strip": "3.0.2",
    "@storybook/addon-essentials": "7.0.12",
    "@storybook/addon-interactions": "7.0.12",
    "@storybook/addon-links": "7.0.12",
    "@storybook/react-vite": "7.0.12",
    "@storybook/theming": "7.0.12",
    "@swc/core": "1.3.58",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/loadable__component": "5.13.4",
    "@types/lodash": "4.14.194",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/coverage-c8": "0.31.1",
    "cypress": "12.16.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.12",
    "jsdom": "22.0.0",
    "patch-package": "7.0.0",
    "pg-promise": "10.11.1",
    "prettier": "2.8.8",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "source-map-explorer": "2.5.3",
    "storybook": "7.0.12",
    "storybook-dark-mode": "3.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-favicon2": "1.1.5",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.1"
  },
  "engines": {
    "node": ">=18.16",
    "npm": ">=9.5"
  },
  "lingui": {
    "locales": [
      "en",
      "fi"
    ],
    "catalogs": [
      {
        "path": "src/locales/{locale}/messages",
        "include": [
          "src"
        ]
      }
    ],
    "format": "po",
    "compileNamespace": "ts"
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "figmage": {
    "outDir": "design-system",
    "tokenize": {
      "tokens": [
        {
          "name": "colors",
          "type": "color"
        },
        {
          "name": "typography",
          "type": "text"
        },
        {
          "name": "shadows",
          "type": "drop-shadow"
        },
        {
          "name": "spacing",
          "type": "width",
          "nodeId": "110:694"
        },
        {
          "name": "sizes",
          "type": "height",
          "nodeId": "102:7"
        },
        {
          "name": "radii",
          "type": "radius",
          "nodeId": "102:8"
        },
        {
          "name": "icons",
          "type": "svg",
          "nodeId": "102:2"
        }
      ]
    },
    "codegen": {
      "defaults": {
        "filetype": "ts",
        "tokenCase": "camel"
      },
      "icons": {
        "filetype": "svg",
        "filename": "icon-sprite",
        "sprite": {
          "writeIds": true,
          "spriteDir": "assets"
        }
      }
    }
  }
}
