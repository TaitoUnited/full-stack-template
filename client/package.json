{
  "type": "module",
  "name": "full-stack-template-client",
  "scripts": {
    "install:clean": "rm -rf node_modules && npm install",
    "pretty-quick": "pretty-quick",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check './src/**/*.{ts,tsx}'",
    "format:write": "prettier --write './src/**/*.{ts,tsx}'",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "prestart": "npm run generate",
    "prebuild": "NODE_ENV=production npm run generate",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "ANALYZE=true npm run build && source-map-explorer 'build/assets/*.js' --no-border-checks",
    "i18n:extract": "lingui extract --clean",
    "uikit:preview": "storybook dev -p 6006",
    "uikit:build": "storybook build",
    "design-system:sync": "npm-run-all -s design-system:tokenize design-system:codegen design-system:format",
    "design-system:tokenize": "figmage tokenize --env ./src/styled-system/.env",
    "design-system:codegen": "figmage codegen --env ./src/styled-system/.env",
    "design-system:format": "prettier --write \"src/styled-system/tokens/*.ts\"",
    "generate": "npm-run-all -p generate:*",
    "generate:graphql": "gql-tada generate-output",
    "generate:styled-system": "panda codegen --clean"
  },
  "dependencies": {
    "@apollo/client": "4.0.4",
    "@internationalized/date": "3.9.0",
    "@lingui/react": "5.4.1",
    "@loadable/component": "5.16.7",
    "@sentry/browser": "10.8.0",
    "@tanstack/react-router": "1.131.31",
    "@tanstack/zod-adapter": "1.131.31",
    "@wry/equality": "0.5.7",
    "date-fns": "2.29.3",
    "gql.tada": "1.8.13",
    "graphql": "16.11.0",
    "lodash": "4.17.21",
    "motion": "12.23.12",
    "react": "19.1.1",
    "react-aria": "3.43.1",
    "react-aria-components": "1.12.1",
    "react-dom": "19.1.1",
    "react-merge-refs": "2.1.1",
    "react-stately": "3.41.0",
    "react-use-measure": "2.1.7",
    "rxjs": "7.8.2",
    "sonner": "2.0.7",
    "spin-delay": "2.0.1",
    "zod": "4.1.5",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.52.9",
    "@eslint/compat": "1.3.2",
    "@eslint/js": "9.34.0",
    "@lingui/cli": "5.4.1",
    "@lingui/format-po": "5.4.1",
    "@lingui/vite-plugin": "5.4.1",
    "@optimize-lodash/rollup-plugin": "5.0.2",
    "@pandacss/dev": "0.53.7",
    "@peterek/vite-plugin-favicons": "2.1.0",
    "@rollup/plugin-strip": "3.0.4",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-links": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@storybook/theming": "8.4.7",
    "@tanstack/router-devtools": "1.131.32",
    "@tanstack/router-plugin": "1.131.32",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/loadable__component": "5.13.9",
    "@types/lodash": "4.17.14",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.42.0",
    "@typescript-eslint/parser": "8.42.0",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/coverage-v8": "3.2.4",
    "babel-plugin-macros": "3.1.0",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-lingui": "0.11.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "figmage": "1.1.0",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "source-map-explorer": "2.5.3",
    "storybook": "8.4.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "vite": "7.1.4",
    "vite-plugin-splash-screen": "0.2.0",
    "vite-plugin-watch-and-run": "1.7.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  }
}
